{
  "name": "tfind",
  "version": "1.0.0",
  "description": "Find missing people.",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/jest",
    "postinstall": "node_modules/.bin/browserify -t babelify client/main.js -o ./public/js/bundle.js",
    "start": "node_modules/.bin/babel-node  --presets es2015,react index.js",
    "babelnode": "node_modules/.bin/babel-node  --presets es2015,react index.js",
    "prebabelnode": "node_modules/.bin/browserify -t babelify client/main.js -o ./public/js/bundle.js",
    "build:css": "node_modules/.bin/stylus -u nib stylus/tfind.styl -o public/css/tfind.css",
    "watch:css": "node_modules/.bin/stylus -w -u nib stylus/tfind.styl -o public/css/tfind.css",
    "build": "npm run build:css && node_modules/.bin/browserify -t babelify client/main.js -o ./public/js/bundle.js",
    "local": "npm run build && npm run watch:css & npm start",
    "watch": "npm run watch:css &  nodemon -e jsx,js,json -w ./index.js -w ./components -w ./resources -w ./client --exec npm run babelnode"
  },
  "jest": {
    "testDirectoryName": "tests",
    "testPathDirs": [
      "tests"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6",
      "jsx"
    ],
    "scriptPreprocessor": "preprocessor.js",
    "unmockedModulePathPatterns": [
      "components",
      "node_modules/react",
      "node_modules/react-bootstrap",
      "node_modules/react-bootstrap-datetimepicker",
      "node_modules/react-dom",
      "node_modules/react-addons-test-utils"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "testFileExtensions": [
      "es6",
      "js",
      "jsx"
    ]
  },
  "engines": {
    "node": "0.11.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/semitaho/tfind.git"
  },
  "author": "Toni Aho",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/semitaho/tfind/issues"
  },
  "homepage": "https://github.com/semitaho/tfind",
  "dependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.1.21",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^6.4.0",
    "body-parser": "^1.14.1",
    "browserify": "^11.2.0",
    "express": "4.13.3",
    "flux": "^2.1.1",
    "jade": "1.11.0",
    "jquery": "^2.1.4",
    "mongodb": "^2.0.47",
    "multer": "^1.1.0",
    "nib": "1.1.0",
    "nodemon": "^1.8.0",
    "react": "^0.14.3",
    "react-bootstrap": "^0.27.3",
    "react-bootstrap-datetimepicker": "0.0.21",
    "react-dom": "^0.14.3",
    "stylus": "0.52.4"
  },
  "devDependencies": {
    "babel-jest": "*",
    "jest-cli": "*",
    "react-addons-test-utils": "^0.14.0"
  }
}
